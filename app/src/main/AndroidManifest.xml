<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.computershop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AccessoriesDetail"></activity>
        <activity android:name=".ViewOrder" />
        <activity android:name=".Orderplaced" />
        <activity android:name=".Payment" />
        <activity android:name=".AccessoriesList" />
        <activity android:name=".LaptopList" />
        <activity android:name=".LaptopTypes" />
        <activity android:name=".Desktoplist" />
        <activity android:name=".ComputerDetail" />
        <activity android:name=".Popularlist" />
        <activity android:name=".ComputerTypes" />
        <activity android:name=".Category" />
        <activity android:name=".usersignup" />
        <activity android:name=".MainActivity" />
        <activity android:name=".home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>